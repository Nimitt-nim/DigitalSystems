    "def float_to_binary_IEEE754(f):\n",
    "    packed = struct.pack('>f', f)\n",
    "    binary = ''.join(format(byte, '08b') for byte in packed)\n",
    "    \n",
    "    return binary\n",
    "print(float_to_binary_IEEE754(0.14))"
